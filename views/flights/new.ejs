<%- include('../partials/html-head')  %> 
<link rel="stylesheet" href="/stylesheets/flights/new.css" />
<%- include('../partials/nav')  %>

<main>
  <h1><%= title %></h1>
  <form id="new-form" action="/flights" method="POST">
    <label for="title-input">Airline:</label>
    <input type="text" name="airline" id="airline">
    <label for="airport">Airport</label>
    <input type="text" name="airport" id="airport">
    <label for="flightNo">FlightNo:</label>
    <input type="text" name="flightNo" id="flightNo">
    <label for="departs">Departs:</label>
    <input type="text" name="departs" id="departs">
    <button type="submit">Add Flight</button>
  </form>
</main>

