<%- include('../partials/html-head') %>
<link rel="stylesheet" href="/stylesheets/flightss/index.css" />
<%- include('../partials/nav') %>

<main>
  <h1>Flight List</h1>
  <table>
    <thead>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>FlightNo</th>
        <th>Departs</th>
      </tr>
    </thead>
    <tbody>
      <% flights.forEach(movie => { %>
        <tr>
          <td><%= movie.title %></td>
          <td><%= movie.releaseYear %></td>
          <td><%= movie.mpaaRating %></td>
          <td><%= movie.cast.join(', ') %></td>
          <%# finish the ternary expression to display 'Yes' or 'Nope' %> 
          <td><%= movie.nowShowing ? 'Yes' : 'Nope' %></td>
        </tr>
      <%# Close the forEach using EJS here %>
      <% }) %>
    </tbody>
  </table>
</main>

<%- include('../partials/footer') %>